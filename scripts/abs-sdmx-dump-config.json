        "description": "Age 5-9"
      }, {
        "code": "A10",
        "name": "10to14",
        "description": "Age 10-14"
      }, {
        "code": "A15",
        "name": "15to19",
        "description": "Age 15-19"
      }, {
        "code": "A20",
        "name": "20to24",
        "description": "Age 20-24"
      }, {
        "code": "A25",
        "name": "25to29",
        "description": "Age 25-29"
      }, {
        "code": "A30",
        "name": "30to34",
        "description": "Age 30-34"
      }, {
        "code": "A35",
        "name": "35to39",
        "description": "Age 35-39"
      }, {
        "code": "A40",
        "name": "40to44",
        "description": "Age 40-44"
      }, {
        "code": "A45",
        "name": "45to49",
        "description": "Age 45-49"
      }, {
        "code": "A50",
        "name": "50to54",
        "description": "Age 50-54"
      }, {
        "code": "A55",
        "name": "55to59",
        "description": "Age 55-59"
      }, {
        "code": "A60",
        "name": "60to64",
        "description": "Age 60-64"
      }, {
        "code": "A65",
        "name": "65to69",
        "description": "Age 65-69"
      }, {
        "code": "A70",
        "name": "70to74",
        "description": "Age 70-74"
      }, {
        "code": "A75",
        "name": "75to79",
        "description": "Age 75-79"
      }, {
        "code": "A80",
        "name": "80to84",
        "description": "Age 80-84"
      }, {
        "code": "8599",
        "name": "85andover",
        "description": "Age 85+"
      }, {
        "code": "TT",
        "name": "all",
        "description": "All Ages"
      }]
    },
    {
      "name": "regionType",
      "values": [{
        "code": "SA2",
        "name": "sa2",
        "description": "Statistical Area Level 2"
      },
      {
        "code": "SA3",
        "name": "sa3",
        "description": "Statistical Area Level 3"
      }]
    },
    {
      "name": "region",
      "values": [{
        "code": "",
        "name": "all",
        "description": "All Regions"
      }]
    },
    {
      "name": "frequency",
      "values": [{
        "code": "A",
        "name": "annual",
        "description": "Annual"
      }]
    }
  ],
  "periods": [
    "2015"
  ]
